{
  "name": "react-starter",
  "version": "0.0.1",
  "description": "React Starter Kit with Webpack + Babel + Tape",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js --progress --profile --colors",
    "dev": "webpack-dev-server --progress --profile --colors --hot",
    "coverage": "nyc -e .jsx tape --require babel-register tests/*.jsx",
    "test": "tape --require babel-register tests/*.jsx | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sogko/react-starter.git"
  },
  "keywords": [
    "react",
    "babel",
    "webpack",
    "starter",
    "tape",
    "es6",
    "es2015"
  ],
  "author": "Hafiz Ismail",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sogko/react-starter/issues"
  },
  "homepage": "https://github.com/sogko/react-starter#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "faucet": "0.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "nyc": "^9.0.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "tape": "^4.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
